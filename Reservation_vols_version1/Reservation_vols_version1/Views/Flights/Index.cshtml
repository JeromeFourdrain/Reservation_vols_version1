@{
    List<Flight> flights = (List<Flight>)ViewData["flights"];
}

<h1>Liste des vols :</h1>

<!-- <ul> Début de la liste -->
<ul>
    @foreach(Flight flight in flights)
    {
        <!-- <li> Element de la liste </li> -->
        <li>Numéro du vol : @flight.FlightId</li>
    }

<!-- </ul> Fin de la liste-->
</ul>